package com.hexa.entity;

import java.time.LocalDate;

public class Emp {

	private int eid;
	private String ename;
	private double sal;
	private String dept;
	private LocalDate doj;
	
	public Emp(int eid, String ename, double sal, String dept, LocalDate doj) {
		super();
		this.eid = eid;
		this.ename = ename;
		this.sal = sal;
		this.dept = dept;
		this.doj = doj;
	}
	
	
	
	
}
